Algoritmo "semnome"
// Disciplina   : [Linguagem e Lógica de Programação]

var
   tabuada : REAL
inicio

      tabuada := 0
      ESCREVA ("Digite a tabuada desejada: ")
      LEIA (tabuada)

      ENQUANTO 1 < 10 FACA
          tabuada <- tabuada + 1
      FIMENQUANTO
      ESCREVAL ("Resultado: ", tabuada)

Fimalgoritmo